  "description": "A custom <select> for Bootstrap using button dropdown as replacement",
  "type": "component",
  "keywords": [
    "form",
    "bootstrap",
    "select",
    "replacement"
  ],
  "require": {
    "components/jquery": ">=1.8"
  },
  "homepage": "http://silviomoreto.github.io/bootstrap-select",
  "authors": [
    {
      "name": "Silvio Moreto",
      "homepage": "https://github.com/silviomoreto"
    }
  ],
  "license": "MIT",
  "suggest": {
    "components/bootstrap": "~3.0.0"
  },
  "extra": {
    "component": {
      "scripts": [
        "dist/js/bootstrap-select.js"
      ],
      "files": [
        "dist/js/*.js",
        "dist/js/i18n/*.js",
        "dist/js/*.map",
        "dist/css/*.css",
        "dist/css/*.map",
        "less/*.less"
